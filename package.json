{
  "name": "line-awesome",
  "version": "1.0.2",
  "description": "Line Awesome by Icon8, packaged in scss. A drop-in replacement for FontAwesome.",
  "scripts": {
    "dev": "./node_modules/.bin/node-sass src/line-awesome.scss > css/line-awesome.css",
    "prod": "./node_modules/.bin/node-sass --source-comments false --output-style compressed src/line-awesome.scss > css/line-awesome.min.css"
  },
  "main": "src/line-awesome.scss",
  "style": "css/line-awesome.css",
  "keywords": [
    "FontAwesome",
    "LineAwesome",
    "Icon8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alsofronie/line-awesome.git"
  },
  "homepage": "https://github.com/alsofronie/line-awesome",
  "author": "Alex Sofronie <alsofronie@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.7.2"
  }
}
