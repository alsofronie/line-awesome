{
  "name": "line-awesome",
  "version": "1.1.3",
  "description": "Line Awesome by Icon8, packaged in scss",
  "scripts": {
    "dev": "./node_modules/.bin/node-sass src/scss/line-awesome.scss > css/line-awesome.css",
    "prod": "./node_modules/.bin/node-sass --source-comments false --output-style compressed src/scss/line-awesome.scss > css/line-awesome.min.css"
  },
  "style": "css/line-awesome.css",
  "keywords": [
    "FontAwesome",
    "LineAwesome",
    "Icon8"
  ],
  "author": "Alex Sofronie <alsofronie@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.7.2"
  }
}
